  <Param tooltip="if true, the unreferenced vertices remaining after the face deletion are removed." name="removeUnref" type="RichBool" value="true" description="Remove unfreferenced vertices"/>
 </filter>
 <filter name="Convex Hull"/>
</FilterScript>
